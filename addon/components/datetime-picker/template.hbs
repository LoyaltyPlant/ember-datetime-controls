{{date-control
    date=date
    minDate=minDate
    maxDate=maxDate
    disabledDates=disabledDates
    timeZone=timeZone
    locale=locale
    onChange=(action "onDateChange")
    format=format
}}

{{time-control
    time=time
    timeZone=timeZone
    locale=locale
    onchange=(action "onTimeChange")
    disabled=isTimePickerDisabled
}}