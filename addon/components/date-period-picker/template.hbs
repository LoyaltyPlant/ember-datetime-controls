{{!--from--}}
{{date-picker
    date=dateFrom
    minDate=minDateFrom
    maxDate=maxDateFrom
    disabledDates=disabledDatesFrom
    onChange=(action "onDateFromChange")
    format=format
    size=size
}}
{{!--to--}}
{{date-picker
    date=dateTo
    minDate=minDateTo
    maxDate=maxDateTo
    disabledDates=disabledDatesTo
    onChange=(action "onDateToChange")
    format=format
    size=size
}}
