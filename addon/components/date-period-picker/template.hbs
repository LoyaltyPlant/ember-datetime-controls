{{!--from--}}
{{date-picker
    date=dateFrom
    minDate=minDateFrom
    maxDate=maxDateFrom
    disabledDates=disabledDatesFrom
    onChange=(action onDateFromChange)
    disabledDates=disabledDatesFrom
    timeZone=timeZone
    locale=locale
    format=format
    size=size
    allowClear=allowClear
}}
{{!--to--}}
{{date-picker
    date=dateTo
    minDate=minDateTo
    maxDate=maxDateTo
    disabledDates=disabledDatesTo
    onChange=(action onDateToChange)
    disabledDates=disabledDatesTo
    timeZone=timeZone
    locale=locale
    format=format
    size=size
    allowClear=allowClear
}}
